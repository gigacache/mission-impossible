{
    "name": "gigacache/mission-impossible",
    "description": "A Symfony cli tool to quickly and efficiently track missions from a json file. Built for a custom event-driven middleware system.",
    "type": "library",
    "license": "MIT License",
    "authors": [
      {
        "name": "gigacache",
        "homepage": "https://gigacache.github.io/"
      }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "friendsofphp/php-cs-fixer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mission\\Impossible\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Mission\\Impossible\\Test\\": "test"
        }
    },
    "minimum-stability": "dev"
}
